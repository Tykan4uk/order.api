#docker
docker-compose build --no-cache
docker-compose up   
   
#Add-Migration
dotnet ef --startup-project services/OrderApi.Service migrations add InitialMigration --project src/OrderApi

#Update-Migration
dotnet ef --startup-project services/OrderApi.Service database update InitialMigration --project src/OrderApi

#Remove-Migration
dotnet ef --startup-project services/OrderApi.Service migrations remove --project src/OrderApi -f